#include <stdio.h>
int main()
{
int a[10][10];
int m,n,i,j,s1=0;
printf("Enter rows and columns of matrix=");
scanf("%d%d"&m,&n);
for(i=0;i<m;i++)
 {
  for(j=0;j<n;j++)
   { 
    printf("nEnter Number=");
    scanf("%d",&a[i][j]);
    }
 }
 for(i=0;i<m;i++)
  {
    for(j=0;j<n;j++)
     {
       printf("%d",a[i][j]);
     }
     printf("\n");
  }
  for(i=0;i<m;i++)
    {
     for(j=0;j<n;j++)
       {
          s1=s1+a[i][j];
       }
   }
   printf("\nSum of all array elements=%d",s1);
   return 0;
}
